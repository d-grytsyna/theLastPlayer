<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cardgame.SavedController">
   <children>
      <ImageView fitHeight="721.0" fitWidth="933.0" layoutX="-22.0" layoutY="-8.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@gameStyle/GameBack.png" />
         </image>
      </ImageView>
      <Label layoutX="260.0" layoutY="54.0" prefHeight="98.0" prefWidth="393.0" text="The last player" textFill="WHITE">
         <font>
            <Font name="Zapf Dingbats" size="57.0" />
         </font>
      </Label>
      <Button fx:id="NewGame" layoutX="350.0" layoutY="260.0" mnemonicParsing="false" onAction="#NewGame" prefHeight="65.0" prefWidth="191.0" style="-fx-background-color: #836786#836786 #836786#836786;" text="NEW GAME" textFill="WHITE">
         <font>
            <Font size="23.0" />
         </font>
      </Button>
      <Button fx:id="Saved" layoutX="350.0" layoutY="400.0" mnemonicParsing="false" onAction="#SavedGame" prefHeight="65.0" prefWidth="191.0" style="-fx-background-color: #836786#836786 #836786#836786;" text="SAVED" textFill="WHITE">
         <font>
            <Font size="23.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
